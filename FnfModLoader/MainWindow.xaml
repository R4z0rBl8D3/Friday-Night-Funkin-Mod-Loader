<Window x:Class="FnfModLoader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FnfModLoader"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="690">
    <Grid>
        <StatusBar HorizontalAlignment="Left" Height="26" Margin="0,383,0,0" VerticalAlignment="Top" Width="792">
            <Label x:Name="statusLbl" Content="Status: Waiting..." Height="26"/>
        </StatusBar>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="344" Margin="0,39,0,0" VerticalAlignment="Top" Width="677"/>
        <Button x:Name="playBtn" Content="Play" HorizontalAlignment="Left" Height="29" Margin="587,10,0,0" VerticalAlignment="Top" Width="90" Click="playBtn_Click"/>
        <Label Content="Friday Night Funkin' Week Loader" HorizontalAlignment="Left" VerticalAlignment="Top" Height="39" Width="542" FontSize="25"/>


    </Grid>
</Window>
